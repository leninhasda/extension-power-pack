{
  "manifest_version": 3,
  "name": "Gmail Shortcut PowerPack",
  "version": "0.0.1",
  "description": "Provides useful gmail shortcut bundles",
  "author": "Lenin Hasda <leninhasda@gmail.com>",
  "icons": {
    "48": "icon-128.png"
  },
  "action": {
    "default_title": "Gmail Shortcut"
  },
  "background": {},
  "permissions": [],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}